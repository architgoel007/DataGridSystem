    <script>
        $(document).ready(function () {
            $('ul#myTab li a[data-toggle="tab"]').on('shown.bs.tab', function (e) {

                var href = $(e.target).attr('href');
                var $curr = $("ul#myTab  a[href='" + href + "']").parent();

                $('ul#myTab li').removeClass();
                $curr.addClass("active");
                $curr.prevAll().addClass("visited");

            });
        });
    </script>
    <script>
        $('.btnNext').click(function () {
            $('.nav-tabs > .active').next('li').find('a').trigger('click');
        });

        $('.btnPrevious').click(function () {
            $('.nav-tabs > .active').prev('li').find('a').trigger('click');
        });
    </script>
    <script>
        $('ul.product-listing li').click(function () {
            $('ul.product-listing li.selected').removeClass('selected');
