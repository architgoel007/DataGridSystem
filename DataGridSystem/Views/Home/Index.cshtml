    <script>
        $('document').ready(function () {

            var $file = $('#upload-menu'),
                $label = $file.next('label'),
                $labelText = $label.find('span'),
                $labelRemove = $('i.remove'),
                labelDefault = $labelText.text();

            // on file change
            $file.on('change', function (event) {
                $("#upload-menu").closest(".upload-menu-section").addClass("uploaded");
                var fileName = $file.val().split('\\').pop();
                if (fileName) {
                    console.log($file)
                    $labelText.text(fileName);
                    $labelRemove.show();
                } else {
                    $labelText.text(labelDefault);
                    $labelRemove.hide();
                }
            });

            // Remove file
            $labelRemove.on('click', function (event) {
                $("#upload-menu").closest(".upload-menu-section").removeClass("uploaded");
                $file.val("");
                $labelText.text(labelDefault);
                $labelRemove.hide();
                console.log($file)
            });
        })
    </script>
